            <string>///camera code + shaking
var FullScreen, SkipShake;
FullScreen = window_get_fullscreen();

ShakePower -= ShakeDecayRate;
SkipShake = false;

//skip the shake if it's not needed.
if (ShakeMode == shakemodes.shake_off) or (ShakePower &lt;= 0) {
    SkipShake = true;
    //also, get the window position
    if FullScreen = false {
        WindowX = window_get_x();
        WindowY = window_get_y(); 
        }
    }

//set the view position
if instance_exists(obj_player) {
    view_xview[0] = clamp(obj_player.x -(view_wview[0] * 0.5), 0, (room_width -view_wview[0]));
    view_yview[0] = clamp(obj_player.y -(view_hview[0] * 0.5), 0, (room_height -view_hview[0]));
    }
   
//shake it
if SkipShake == false {
    scr_camera_shake(ShakePower);
    }

//set it all back to normal
if (ShakePower &lt;= 0) and not FullScreen {
    window_set_position(WindowX, WindowY); 
    }

</string>
